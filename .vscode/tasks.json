{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Executar testes com npm test",
			"type": "shell",
			"command": "npm test",
			"problemMatcher": [
				"$mocha"
			],
			"group": "test"
		},
		{
			"label": "K6: Run Current File with Report",
			"type": "shell",
			"command": "bash",
			"args": [
				"run_with_report.sh",
				"${file}",
				"${fileBasenameNoExtension}-report.html"
			],
			"group": "test"
		}
	]
}